generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Teachers {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  password String
  School   String
}

model Students {
  id       Int     @id @default(autoincrement())
  name     String
  school   String
  class    String
  password String?
}

model Admins {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  password String
}

model Nominations {
  id       Int     @id @default(autoincrement())
  author   String
  nominee  String
  content  String
  votes    Int
  approved Boolean
}

model TeacherClasses {
  ID      Int     @id @default(autoincrement())
  Teacher String
  Class   String
  school  String?
}
